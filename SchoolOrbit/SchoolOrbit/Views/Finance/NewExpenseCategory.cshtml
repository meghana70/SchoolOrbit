@model SchoolOrbit.Models.ExpenseType

@{
    ViewBag.Title = "NewExpenseCategory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <h1>
        Create Expense Category
        <small></small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-user"></i> Expense Category</a></li>
        <li class="active">New</li>
    </ol>
</section>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <section class="content">
        <div class="box">
            <div class="box-header with-border">
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-5">
                            <table class="table table-bordered table-striped">
                                <tr>
                                    <td>
                                        <div class="form-group">
                                            @Html.LabelFor(model => model.expense_type, htmlAttributes: new { @class = "control-label col-md-3" })
                                            <div class="col-md-4">
                                                @Html.EditorFor(model => model.expense_type, new { htmlAttributes = new { @class = "form-control" } })
                                                @Html.ValidationMessageFor(model => model.expense_type, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                               <tr>
                                   <td>
                                       <div class="form-group">
                                           @Html.LabelFor(model => model.isactive, htmlAttributes: new { @class = "control-label col-md-3" })
                                           <div class="col-md-4">
                                               <div class="">
                                                   @Html.EditorFor(model => model.isactive)
                                                   @Html.ValidationMessageFor(model => model.isactive, "", new { @class = "text-danger" })
                                               </div>
                                           </div>
                                       </div>
                                   </td>
                               </tr>

                     <tr>
                       
                         <td >
                             <div class="form-group">
                                 <div class="col-md-10">
                                     <input type="submit" value="Create" class="btn btn-primary" />
                                     <a href="ExpenseList" class="btn btn-info"> Back</a>
                                 </div>

                             </div>
                         </td>

                     </tr>           
</table>

                        </div>
                        </div>
                    </div>
                </div>
            </div>

</section>
}