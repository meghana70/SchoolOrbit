@model SchoolOrbit.Models.ExpenseType

@{
    ViewBag.Title = "ExpTypeDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <h1>
        Expense Category
        <small>Details</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-users"></i>Expense Category</a></li>
        <li class="active">Details</li>
    </ol>
</section>

<section class="content">
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">
                Expense Category Details [
                @Html.ActionLink("Edit", "EditExpType", new { id = Model.id })
                ]
            </h3>
            <table class=" table table-bordered ">
                <tr>
                    <td class="text-bold">
                        @Html.DisplayNameFor(model => model.expense_type)
                    </td>
                    <td >
                        @Html.DisplayFor(model => model.expense_type)
                    </td>
                </tr>
                <tr>
                    <td class="text-bold">
                        @Html.DisplayNameFor(model=>model.isactive)
                    </td>
                    <td>
                        @Html.DisplayFor(model=>model.isactive)
                    </td>
                </tr>
            </table>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <a href="../ExpenseList" class="btn btn-info">Back to List</a>
                </div>
            </div>
        </div>            
   </div>
</section>
